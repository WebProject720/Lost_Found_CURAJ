---
import  axios  from "axios";
import {SERVER} from '../../constants.astro'
const route = "/users";

type data={
    username:String,
    enroll:String,
    password:String
}
export const RegisterRequest = async ({username,enroll,password}:data) => {
    console.log("Server",{username,enroll,password});
    
    try {
        await axios.post(SERVER + '/users/register',{username,enroll,password}).then((response) => {
            console.log(response);
            return { status: true, data: response, msg: 'Registration Successful' }
        }).catch((error) => {
            console.log(error);
            return {
                status: false,
                error: error,
                msg: 'Registration Failed !'
            }
        })
    } catch (error) {
        console.log(error);
        return {
            status: false,
            error: error,
            msg: 'Registration Failed !'
        }
    }
}
